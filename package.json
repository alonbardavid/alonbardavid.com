  "scripts": {
    "build": "cross-env NODE_ENV=production BUILD_TASK=build webpack --display-chunks --progress --bail",
    "build-dev": "cross-env NODE_ENV=development BUILD_TASK=build webpack --display-chunks --progress --bail",
    "profile-size": "cross-env NODE_ENV=production BUILD_TASK=profile webpack --profile --json |  webpack-bundle-size-analyzer",
    "clean": "del-cli ./target",
    "dev": "cross-env NODE_ENV=development BUILD_TASK=dev webpack-dev-server --progress --watch-poll",
    "prebuild": "npm run clean",
    "start": "npm run dev"
  },
